{
  "name": "@1shotapi/wallet",
  "version": "0.0.1",
  "description": "SDK for embedding the 1ShotPay passkey wallet in your site",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:test": "vite build",
    "build:test:gh-pages": "vite build",
    "dev": "vite",
    "dev:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "clean": "rimraf dist dist-test docs"
  },
  "keywords": [
    "1shotapi",
    "wallet",
    "passkey",
    "1shotpay",
    "sdk"
  ],
  "author": "1Shot API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1shotapi/wallet.git"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.1.1",
  "devDependencies": {
    "@types/postmate": "^1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "postmate": "^1.5.2",
    "safe-stable-stringify": "^2.5.0",
    "ts-brand": "^0.2.0"
  }
}
